<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.yang.questionnaire.mapper.OptionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="pers.yang.questionnaire.entity.Option">
        <id column="id" property="id" />
        <result column="questionnaire_id" property="questionnaireId" />
        <result column="content" property="content" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, questionnaire_id, content
    </sql>

</mapper>
